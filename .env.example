# Environment Configuration Example
# Copy this file to .env.local and fill in your values

# Application Environment
VITE_APP_ENV=development
NODE_ENV=development

# API Configuration (if needed in future)
# VITE_API_BASE_URL=https://api.liftfire.app
# VITE_API_VERSION=v1

# Analytics (if implemented)
# VITE_GA_TRACKING_ID=G-XXXXXXXXXX
# VITE_HOTJAR_ID=XXXXXXX

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=false
VITE_ENABLE_PERFORMANCE_MONITORING=false

# Internationalization
VITE_DEFAULT_LANGUAGE=en
VITE_SUPPORTED_LANGUAGES=en,es

# Build Configuration
VITE_BUILD_TIMESTAMP=
VITE_BUILD_VERSION=
VITE_BUILD_COMMIT_SHA=

# External Services
# VITE_DISCORD_INVITE_URL=https://discord.gg/liftfire
# VITE_GITHUB_REPO_URL=https://github.com/liftfire/app
# VITE_SUPPORT_EMAIL=support@liftfire.app

# Development Only
VITE_DEBUG_MODE=false
VITE_MOCK_API=false